{
  "name": "silent-spirit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite & vite --config vite.docs.config.ts",
    "dev:app": "vite",
    "docs": "vite --config vite.docs.config.ts",
    "dev:e2e": "chmod +x scripts/dev-e2e.sh && ./scripts/dev-e2e.sh",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:biome": "biome lint .",
    "format": "biome format --write .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test": "vitest run",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "typecheck": "tsc -b",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.162.4",
    "@tanstack/router-devtools": "^1.162.4",
    "@tanstack/router-plugin": "^1.162.4",
    "clsx": "^2.1.1",
    "immer": "^11.1.4",
    "lucide-react": "^0.575.0",
    "mermaid": "^11.12.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "overrides": {
    "minimatch": ">=10.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.2.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "knip": "^5.85.0",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings 0"
  }
}